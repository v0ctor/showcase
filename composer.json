{
    "name": "v0ctor/showcase",
    "license": "MIT",
    "type": "project",
    "homepage": "https://v0ctor.me",
    "authors": [
        {
            "name": "Víctor Díaz Marco",
            "email": "victor@diazmarco.me",
            "homepage": "https://v0ctor.me"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-intl": "*",
        "fideloper/proxy": "^4.2",
        "htmlmin/htmlmin": "^6.0",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^1.0",
        "mariuzzo/laravel-js-localization": "^1.4"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "nunomaduro/collision": "^3.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "[ -f vendor/bin/artisan ] || ln -s ../../artisan vendor/bin/artisan",
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    }
}
